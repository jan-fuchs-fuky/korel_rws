#!/bin/bash

REV=$(svnversion -n .)
LAST_CHANGED_DATE=$(svn info -r ${REV%M*} |fgrep "Last Changed Date:")

cat <<EOF >lib/svnversion.py
#!/usr/bin/env python

# Generated automatically from ../make.svnversion. DO NOT EDIT.

def get():
    return "$REV"

def date():
    return "$LAST_CHANGED_DATE"
EOF
